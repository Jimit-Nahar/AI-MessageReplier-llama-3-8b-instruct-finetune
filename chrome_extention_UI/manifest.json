{
  "manifest_version": 3,
  "name": "Sample Chrome Extension",
  "version": "1.0",
  "action": {
      "default_popup": "home.html"
  },
  "permissions": [
      "activeTab",
      "scripting"
  ],
  "web_accessible_resources": [
      {
          "resources": ["libs/html2canvas.min.js", "libs/tesseract.min.js"],
          "matches": ["<all_urls>"]
      }
  ]
}
